
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Movie {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  overview String
  poster String
  image String
  trailer String
  isFeatured Boolean @default(false)
  createdAt DateTime @default(now())
}
